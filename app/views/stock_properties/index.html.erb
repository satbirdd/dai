<h1>Listing stock_properties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_properties.each do |stock_property| %>
      <tr>
        <td><%= link_to 'Show', stock_property %></td>
        <td><%= link_to 'Edit', edit_stock_property_path(stock_property) %></td>
        <td><%= link_to 'Destroy', stock_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock property', new_stock_property_path %>
